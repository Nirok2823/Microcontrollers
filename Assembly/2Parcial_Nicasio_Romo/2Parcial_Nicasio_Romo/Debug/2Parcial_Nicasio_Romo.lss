000006 940c 0167                 jmp TIM2_COMP ; Timer2 Compare Handler
000008 940c 0168                 jmp TIM2_OVF ; Timer2 Overflow Handler
00000a 940c 0169                 jmp TIM1_CAPT ; Timer1 Capture Handler
00000c 940c 016a                 jmp TIM1_COMPA ; Timer1 CompareA Handler
00000e 940c 016b                 jmp TIM1_COMPB ; Timer1 CompareB Handler
000010 940c 016c                 jmp TIM1_OVF ; Timer1 Overflow Handler
000012 940c 016d                 jmp TIM0_OVF ; Timer0 Overflow Handler
000014 940c 016e                 jmp SPI_STC ; SPI Transfer Complete Handler
000016 940c 016f                 jmp USART_RXC ; USART RX Complete Handler
000018 940c 0170                 jmp USART_UDRE ; UDR Empty Handler
00001a 940c 0171                 jmp USART_TXC ; USART TX Complete Handler
00001c 940c 0172                 jmp ADC_COMP ; ADC Conversion Complete Handler
00001e 940c 0173                 jmp EE_RDY ; EEPROM Ready Handler
000020 940c 0174                 jmp ANA_COMP ; Analog Comparator Handler
000022 940c 0175                 jmp TWSI ; Two-wire Serial Interface Handler
000024 940c 0176                 jmp EXT_INT2 ; IRQ2 Handler
000026 940c 0177                 jmp TIM0_COMP ; Timer0 Compare Handler
000028 940c 0178                 jmp SPM_RDY ; Store Program Memory Ready Handler
                                 
